%ul(class="nav nav-tabs")
  %li
    %a{:href => "/upload/#{current_upload.id}?file=/mnt/pdfprocess/#{current_upload.agent_id}/#{current_upload.fileName}"}Text
  %li
    %a{:href => "/upload/#{current_upload.id}?file=/mnt/pdfprocess/#{current_upload.agent_id}/#{current_upload.fileName}&page="}View
  %li
    %a{:href => "/upload/#{current_upload.id}?file=/mnt/pdfprocess/#{current_upload.agent_id}/#{current_upload.fileName}&img="}Images
-if @s
  #output
    -@s.each do |line|
      .alert.draggable
        =line
-if @uploaded_pdf
  %iframe{ :allowscriptaccess => 'always', :allowFullScreen => 'false', :frameborder => '0', :height => '780', :width => '100%', :src => "http://docs.google.com/viewer?url=http%3A%2F%2Ffiles.propertyline.com%2Fpdfprocess%2F#{@agent.id}%2F#{File.basename(@uploaded_pdf)}&embedded=true" }
-if @extracted_imgs
  -@extracted_imgs.each do |exim|
    .draggable-photo.ui-widget-content
      %img{ :src => "../images/pdfprocess/#{current_upload.agent_id}/images/#{File.basename(exim)}", :width => 200 }